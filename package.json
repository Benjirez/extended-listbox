{
  "name": "extended-listbox",
  "version": "2.0.0",
  "description": "A simple to use jQuery plugin as powerful alternative to the HTML <select> tag",
  "main": "dist/js/extended-listbox.js",
  "typings": "dist/extended-listbox.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-chris/extended-listbox.git"
  },
  "keywords": [
    "listbox",
    "select",
    "multiselect",
    "form",
    "input",
    "ui",
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "author": "Christian Kotzbauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-chris/extended-listbox/issues"
  },
  "homepage": "https://github.com/code-chris/extended-listbox#readme",
  "files": [
    "dist",
    "doc/pages",
    "doc/CHANGELOG.md",
    "doc/ROADMAP.md"
  ],
  "scripts": {
    "clean": "node node_modules/rimraf/bin.js build/out",
    "build:dev": "node node_modules/webpack/bin/webpack.js",
    "build:prod": "node node_modules/webpack/bin/webpack.js -p",
    "build": "npm run clean && npm run build:dev && npm run build:prod",
    "test": "npm run clean && tsc --project tsconfig.test.json && node node_modules/karma/bin/karma start karma.conf.js"
  },
  "devDependencies": {
    "coverage-percentage": "0.0.2",
    "css-loader": "~0.26.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "globby": "~6.1.0",
    "gulp": "^3.9.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-lesshint": "^1.1.0",
    "gulp-tslint": "^4.3.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-qunit": "^0.1.9",
    "karma-sauce-launcher": "^0.3.1",
    "less": "~2.7.1",
    "less-loader": "~2.2.3",
    "phantomjs-prebuilt": "^2.1.7",
    "qunitjs": "^1.23.1",
    "require-dir": "^0.3.0",
    "requirejs": "^2.2.0",
    "rimraf": "~2.5.4",
    "ts-loader": "~1.3.3",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "webpack": "~1.14.0",
    "yargs": "^3.32.0"
  },
  "dependencies": {
    "jquery": ">=2.2.4"
  }
